/* pages/me/me.wxss */
.view-container{
  width: 100%;
  height:100%;
  background-color: #FFFFFF;
}
.view-hasLogincontainer{
  width: 100%;
  height:100%;
  background-color: #FFFFFF;
  align-items: center;
  justify-content: center;
  display: flex;
  flex-direction: column
}
.view-head{
  height:220rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.logo_style{
  width:150rpx;
  height:150rpx;
}
.row_line{
  border-bottom: 1rpx solid #EFEFEF;
  margin: 30rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.row-left{
  flex:1;
  flex-direction: row;
  display: flex;
  align-items: center;
}
.row-right{
  width:35rpx;
  height:35rpx;
}
.input{
  margin-left: 30rpx;
  display: inline-block;
}
.btnLogin{
  margin: 40rpx;
  background-color: #00b7e3;
  color: #FFFFFF;
}
.headImage{
  width:160rpx;
  height:160rpx;
  border-radius: 80rpx;
}